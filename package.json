{
    "name": "ngx-composite-lib-workspace",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "build-lib-watch": "ng build ngx-composite-lib --watch --configuration development",
        "build-lib-prod": "ng build ngx-composite-lib --configuration production",
        "publish-lib": "cd dist && cd ngx-composite-lib && npm publish",
        "publish-lib-beta": "cd dist && cd ngx-composite-lib && npm publish --tag beta",
        "publish-lib-alpha": "cd dist && cd ngx-composite-lib && npm publish --tag alpha",
        "pack-lib": "npm run build-lib-prod && cd dist && cd ngx-composite-lib && npm pack",
        "compile": "npm run build-lib-prod && ng build",
        "prepare": "npm run compile",
        "test": "ng test",
        "lint": "eslint \"**/*.{ts,tsx}\" ",
        "lint-fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
        "storybook": "npm run docs:json && start-storybook -s .storybook/public -p 6007 --no-manager-cache",
        "build-storybook": "npm run docs:json && build-storybook",
        "chromatic": "chromatic --exit-zero-on-changes --project-token=CHROMATIC_PROJECT_TOKEN"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "14.0.2",
        "@angular/cdk": "14.0.2",
        "@angular/common": "14.0.2",
        "@angular/compiler": "14.0.2",
        "@angular/core": "14.0.2",
        "@angular/elements": "14.0.2",
        "@angular/flex-layout": "^14.0.0-beta.40",
        "@angular/forms": "14.0.2",
        "@angular/material": "^14.0.2",
        "@angular/material-moment-adapter": "^14.0.2",
        "@angular/platform-browser": "14.0.2",
        "@angular/platform-browser-dynamic": "14.0.2",
        "@angular/router": "14.0.2",
        "@mat-datetimepicker/core": "^9.0.68",
        "@mat-datetimepicker/moment": "^9.0.68",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "@pepperi-addons/data-views": "0.0.2",
        "@pepperi-addons/ngx-lib": "^0.4.1-beta.1",
        "@pepperi-addons/papi-sdk": "^1.29.1",
        "@tweenjs/tween.js": "17.4.0",
        "hammerjs": "^2.0.8",
        "moment": "^2.29.2",
        "ng-gallery": "^5.0.0",
        "ng2-file-upload": "^1.4.0",
        "ngx-quill": "^14.3.0",
        "ngx-signaturepad": "0.0.9",
        "ngx-translate-multi-http-loader": "^3.0.0",
        "ngx-virtual-scroller": "^4.0.3",
        "quill": "^1.3.7",
        "rxjs": "~7.5.0",
        "tslib": "^2.2.0",
        "zone.js": "~0.11.5"
    },
    "devDependencies": {
        "@angular-architects/module-federation": "^14.3.8",
        "@angular-architects/module-federation-tools": "^14.3.10",
        "@angular-devkit/build-angular": "14.0.2",
        "@angular/cli": "^14.0.2",
        "@angular/compiler-cli": "^14.0.2",
        "@compodoc/compodoc": "^1.1.18",
        "@storybook/addon-actions": "^6.4.22",
        "@storybook/addon-essentials": "^6.4.22",
        "@storybook/addon-links": "^6.4.22",
        "@storybook/angular": "^6.4.22",
        "@storybook/builder-webpack5": "^6.4.22",
        "@storybook/manager-webpack5": "^6.4.22",
        "@storybook/preset-scss": "^1.0.3",
        "@types/jasmine": "~3.6.0",
        "@types/quill": "^1.3.10",
        "@types/tween.js": "^17.2.0",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "angular-cli-ghpages": "^1.0.0-rc.1",
        "chromatic": "^6.5.4",
        "css-loader": "^6.7.1",
        "eslint": "^7.27.0",
        "eslint-plugin-storybook": "^0.5.10",
        "jasmine-core": "~3.7.0",
        "karma": "~6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ng-packagr": "^14.0.2",
        "sass": "^1.50.1",
        "sass-loader": "^12.6.0",
        "style-loader": "^3.3.1",
        "typescript": "~4.7.3"
    }
}
