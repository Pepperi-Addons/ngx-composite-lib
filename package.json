    "ngx-quill": "^14.3.0",
    "ngx-signaturepad": "0.0.9",
    "ngx-translate-multi-http-loader": "^3.0.0",
    "ngx-virtual-scroller": "^4.0.3",
    "node-fetch": "^3.2.0",
    "quill": "^1.3.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.16",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
