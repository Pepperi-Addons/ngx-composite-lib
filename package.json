{
  "name": "ngx-composite-lib-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build-lib-watch": "ng build ngx-composite-lib --watch --configuration development",
    "build-lib-prod": "ng build ngx-composite-lib --configuration production",
    "publish-lib": "cd dist && cd ngx-composite-lib && npm publish",
    "publish-lib-beta": "cd dist && cd ngx-composite-lib && npm publish --tag beta",
    "publish-lib-alpha": "cd dist && cd ngx-composite-lib && npm publish --tag alpha",
    "pack-lib": "npm run build-lib-prod && cd dist && cd ngx-composite-lib && npm pack",
    "compile": "npm run build-lib-prod && ng build",
    "prepare": "npm run compile",
    "test": "ng test",
    "lint": "eslint \"**/*.{ts,tsx}\" ",
    "lint-fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6007",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^12.5.3",
    "@angular-architects/module-federation-tools": "^12.5.3",
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/material-moment-adapter": "^12.1.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@hallysonh/ngx-imageviewer": "^2.0.4",
    "@mat-datetimepicker/core": "^6.0.3",
    "@mat-datetimepicker/moment": "^6.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@pepperi-addons/data-views": "0.0.2",
    "@pepperi-addons/ngx-lib": "^0.3.15-beta.11",
    "@pepperi-addons/papi-sdk": "^1.29.1",
    "@tweenjs/tween.js": "17.4.0",
